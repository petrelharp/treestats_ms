  intrahash = {34cbdb7dafd2c6abcdf137840544454a},
  keywords = {Kingmans_coalescent population_genetics},
  number = 2,
  pages = {437--460},
  timestamp = {2019-09-11T00:04:40.000+0200},
  title = {Evolutionary relationship of {DNA} sequences in finite populations
},
  url = {https://www.genetics.org/content/105/2/437.long},
  volume = 105,
  year = 1983
}

@article{mcvean2002genealogical,
  abstract = {The degree of association between alleles at different loci, or linkage disequilibrium, is widely used to infer details of evolutionary processes. Here I explore how associations between alleles relate to properties of the underlying genealogy of sequences. Under the neutral, infinite-sites assumption I show that there is a direct correspondence between the covariance in coalescence times at different parts of the genome and the degree of linkage disequilibrium. These covariances can be calculated exactly under the standard neutral model and by Monte Carlo simulation under different demographic models. I show that the effects of population growth, population bottlenecks, and population structure on linkage disequilibrium can be described through their effects on the covariance in coalescence times.},
  added-at = {2014-01-05T22:38:54.000+0100},
  author = {McVean, G A},
  biburl = {https://www.bibsonomy.org/bibtex/259ead3175570411f8b8b9584de9e78bd/peter.ralph},
  interhash = {9d52ee480e18788b8ab23d19926d6ab1},
  intrahash = {59ead3175570411f8b8b9584de9e78bd},
  journal = {Genetics},
  keywords = {coalescent_theory empirical_genealogy genealogies linkage_disequilibrium},
  month = oct,
  number = 2,
  pages = {987-991},
  pmid = {12399406},
  timestamp = {2014-07-09T21:02:20.000+0200},
  title = {A genealogical interpretation of linkage disequilibrium},
  url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1462283/},
  volume = 162,
  year = 2002
}

@article{slatkin1991inbreeding,
  abstract = {This paper describes the relationship between probabilities of identity by descent and the distribution of coalescence times. By using the relationship between coalescence times and identity probabilities, it is possible to extend existing results for inbreeding coefficients in regular systems of mating to find the distribution of coalescence times and the mean coalescence times. It is also possible to express Sewall Wright's FST as the ratio of average coalescence times of different pairs of genes. That simplifies the analysis of models of subdivided populations because the average coalescence time can be found by computing separately the time it takes for two genes to enter a single subpopulation and time it takes for two genes in the same subpopulation to coalesce. The first time depends only on the migration matrix and the second time depends only on the total number of individuals in the population. This approach is used to find FST in the finite island model and in one- and two-dimensional stepping-stone models. It is also used to find the rate of approach of FST to its equilibrium value. These results are discussed in terms of different measures of genetic distance. It is proposed that, for the purposes of describing the amount of gene flow among local populations, the effective migration rate between pairs of local populations, M, which is the migration rate that would be estimated for those two populations if they were actually in an island model, provides a simple and useful measure of genetic similarity that can be defined for either allozyme or DNA sequence data.},
  added-at = {2012-11-22T04:43:51.000+0100},
  author = {Slatkin, M},
  biburl = {https://www.bibsonomy.org/bibtex/29166922a81b7a3fd45a2b0c084e10c7b/peter.ralph},
  description = {Inbreeding coefficients and coalescence times. [Genet Res. 1991] - PubMed - NCBI},
  interhash = {4bb700ef7cd00a8e0fd800ff9e2b0a9d},
  intrahash = {9166922a81b7a3fd45a2b0c084e10c7b},
  journal = {Genet Res},
  keywords = {F_ST coalescent_theory empirical_genealogy inbreeding_coefficient isolation_by_distance random_walk_on_the_torus spatial_coalescent},
  month = oct,
  number = 2,
  pages = {167-175},
  pmid = {1765264},
  timestamp = {2015-02-18T07:32:41.000+0100},
  title = {Inbreeding coefficients and coalescence times},
  url = {http://www.ncbi.nlm.nih.gov/pubmed/1765264},
  volume = 58,
  year = 1991
}

@article{gillespie1979evolutionary,
  abstract = {Langley and Fitch (1974, 1976) have shown that the pattern of nucleotide substitutions in proteins is inconsistent with a Poisson process with constant rate. From this they conclude that the rate is temporally heterogeneous. It is pointed out in this note that a process which is temporally homogeneous but not a Poisson process is compatible with the data if the coefficient of variation of the time between substitutions is around 1.63. Furthermore, theoretical analysis of samples from neutral phylogenies shows that these samples should not appear to be samples from a Poisson process, but should deviate from a Poisson process in the same direction, though perhaps not to the same extent, as do the data.},
  added-at = {2015-02-18T05:56:59.000+0100},
  author = {Gillespie, John H. and Langley, Charles H.},
