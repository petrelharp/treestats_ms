@ARTICLE{Kreitman1983-xr,
  title    = "Nucleotide polymorphism at the alcohol dehydrogenase locus of
              Drosophila melanogaster",
  author   = "Kreitman, M",
  abstract = "The sequencing of eleven cloned Drosophila melanogaster alcohol
              dehydrogenase (Adh) genes from five natural populations has
              revealed a large number of previously hidden polymorphisms. Only
              one of the 43 polymorphisms results in an amino acid change, the
